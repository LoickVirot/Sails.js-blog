<% for(var i = 0; i < data.length; i++) { %>
  <div class="box">
    <div class="box-image">
    </div>
    <img src="<%= data[i].img %>" alt="header-img" />
    <div class="box-content">
      <h1><%= data[i].title %></h1>
      <p>
        <%= data[i].content %>
      </p>
      <a href="/post/<%= data[i].id %>" class="btn">Show more</a>
    </div>
  </div>
<% } %>
